    "@vitejs/plugin-react": "^4.1.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "vite": "^4.5.0"
  },
  
  "devDependencies": {
    "autoprefixer": "^10.4.16",
